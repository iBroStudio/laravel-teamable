{
    "name": "ibrostudio/laravel-teamable",
    "description": "This is my package laravel-teamable",
    "keywords": [
        "iBroStudio",
        "laravel",
        "laravel-teamable"
    ],
    "homepage": "https://github.com/ibrostudio/laravel-teamable",
    "license": "MIT",
    "authors": [
        {
            "name": "iBroStudio",
            "email": "hello@ibro.studio",
            "role": "Developer"
        }
    ],
    "version": "1.0.0",
    "type": "custom-library",
    "require": {
        "php": "^8.2",
        "ibrostudio/laravel-data-repository": "^1.0",
        "illuminate/contracts": "^11.0",
        "spatie/laravel-package-tools": "^1.14.0",
        "spatie/laravel-sluggable": "^3.5"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^8.0",
        "larastan/larastan": "^2.0.1",
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^2.20",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "IBroStudio\\Teamable\\": "src/",
            "IBroStudio\\Teamable\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IBroStudio\\Teamable\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-laravel-teamable --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IBroStudio\\Teamable\\TeamableServiceProvider"
            ],
            "aliases": {
                "Teamable": "IBroStudio\\Teamable\\Facades\\Teamable"
            }
        },
        "custom-directory-installer": {
            "directory": "packages"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
